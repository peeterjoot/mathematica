Cell["\<\
vsplit <- function(vec,test){
    result <-list();
    current <- list(vec[1]);
    for(i in 2:length(vec)){
    \tif(test(vec[i-1],vec[i])){
    \t\tcurrent <- c(current,vec[i])
    \t}else{
    \t\tresult <- c(result,list(unlist(current)))
    \t\tcurrent <- list(vec[i])
    \t}
    }
    if(length(current) != 0){
    \tresult <- c(result,list(unlist(current)))
    }
    return(result)
};\
\>", "ExternalLanguage",
 CellEvaluationLanguage->"R",
 CellChangeTimes->{{3.797159762773477*^9, 3.7971597653638067`*^9}, 
